var Person = {
    create: function (name, gender) {
        var new_person = Object.create(this);
        new_person.name = name;
        new_person.gender = gender;

        return new_person;
    },

    say_hello: function () {
        console.log("Hello, my name is " + this.name);
    }
};

var zaya = Person.create("zaya", "male");
var ari = Person.create("ari", "female");
zaya.__proto__ = my_proto;

